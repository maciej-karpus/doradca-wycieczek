@using DoradcaWyjazdowWypoczynkowych.Models;
@{
    ViewBag.Title = "Search";
}

<h2>Search</h2>
Twoje rekomendacje to:
<table>
@foreach (var recommendation in @ViewData["recommendations"] as List<KeyValuePair<string, List<Atrakcja>>>)
{
     <tr>
          <td>
               @recommendation.Key.AtrakcjaNazwa
          </td>
          <td>
               @recommendation.Key.Lokalizacja
          </td>
          <td>
               @recommendation.Value
          </td>
     </tr>
}
</table>